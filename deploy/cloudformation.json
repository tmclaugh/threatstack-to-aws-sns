{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Threat Stack service integrations",
  "Resources": {
    "SnsTopic": {
      "Type": "AWS::SNS::Topic",
      "Properties": {
        "TopicName": {
          "Ref": "ThreatStackServiceIntegrationsSnsTopicName"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "0317bf14-5346-46c7-b919-a5fd33590889"
        }
      }
    },
    "SnsTopicPolicy": {
      "Type": "AWS::SNS::TopicPolicy",
      "Properties": {
        "PolicyDocument": {
          "Version": "2008-10-17",
          "Id": "__default_policy_ID",
          "Statement": [
            {
              "Sid": "__default_statement_ID",
              "Effect": "Allow",
              "Principal": {
                "AWS": "*"
              },
              "Action": [
                "SNS:GetTopicAttributes",
                "SNS:SetTopicAttributes",
                "SNS:AddPermission",
                "SNS:RemovePermission",
                "SNS:DeleteTopic",
                "SNS:Subscribe",
                "SNS:ListSubscriptionsByTopic",
                "SNS:Publish",
                "SNS:Receive"
              ],
              "Resource": {
                "Fn::Sub": "arn:aws:sns:${AWS::Region}:${AWS::AccountId}:${ThreatStackServiceIntegrationsSnsTopicName}"
              },
              "Condition": {
                "StringEquals": {
                  "AWS:SourceOwner": {
                    "Ref": "AWS::AccountId"
                  }
                }
              }
            },
            {
              "Sid": "AWSCloudTrailSNSPolicy20150319",
              "Effect": "Allow",
              "Principal": {
                "Service": "cloudtrail.amazonaws.com"
              },
              "Action": "SNS:Publish",
              "Resource": {
                "Fn::Sub": "arn:aws:sns:${AWS::Region}:${AWS::AccountId}:${ThreatStackServiceIntegrationsSnsTopicName}"
              }
            }
          ]
        },
        "Topics": [
          {
            "Ref": "SnsTopic"
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "918184b7-ef10-47ee-8bec-6ebb3e54c9a8"
        }
      },
      "DependsOn": [
        "SnsTopic"
      ]
    },
    "SnsConfirmIamManagedPolicy": {
      "Type": "AWS::IAM::ManagedPolicy",
      "Properties": {
        "Description": "Confirm SNS topic subscription",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "SnsConfirmation",
              "Effect": "Allow",
              "Action": [
                "sns:ConfirmSubscription"
              ],
              "Resource": [
                {
                  "Ref": "SnsTopic"
                }
              ]
            }
          ]
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "ab8bf682-8990-4e79-b51d-d86e4cd8c710"
        }
      }
    },
    "Deployment": {
      "Type": "AWS::ApiGateway::Deployment",
      "Properties": {
        "RestApiId": {
          "Ref": "RestApi"
        },
        "Description": {
          "Fn::Join": [
            " ",
            [
              {
                "Ref": "DeploymentStage"
              },
              "stage"
            ]
          ]
        },
        "StageName": {
          "Ref": "DeploymentStage"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "701f3b5c-a6c2-49fa-b527-78be7ae4e50d"
        }
      },
      "DependsOn": [
        "ProxyAnyMethod"
      ]
    },
    "RootResource": {
      "Type": "AWS::ApiGateway::Resource",
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "576f2766-0467-48b4-99be-b3bbfbb7e177"
        }
      },
      "Properties": {
        "RestApiId": {
          "Ref": "RestApi"
        },
        "ParentId": {
          "Fn::GetAtt": [
            "RestApi",
            "RootResourceId"
          ]
        },
        "PathPart": {
          "Ref": "AppName"
        }
      }
    },
    "ProxyResource": {
      "Type": "AWS::ApiGateway::Resource",
      "Properties": {
        "RestApiId": {
          "Ref": "RestApi"
        },
        "ParentId": {
          "Ref": "RootResource"
        },
        "PathPart": "{proxy+}"
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "8d24687a-d967-4592-8119-129aa60439f7"
        }
      }
    },
    "ProxyAnyMethod": {
      "Type": "AWS::ApiGateway::Method",
      "Properties": {
        "RestApiId": {
          "Ref": "RestApi"
        },
        "ResourceId": {
          "Ref": "ProxyResource"
        },
        "HttpMethod": "ANY",
        "AuthorizationType": "NONE",
        "Integration": {
          "Type": "AWS_PROXY",
          "IntegrationHttpMethod": "POST",
          "Uri": {
            "Fn::Sub": "arn:aws:apigateway:${AWS::Region}:lambda:path/2015-03-31/functions/${LambdaFunction.Arn}/invocations"
          },
          "CacheKeyParameters": [
            "method.request.path.proxy"
          ],
          "IntegrationResponses": [
            {
              "ResponseTemplates": {
                "application/json": ""
              },
              "StatusCode": "200"
            }
          ]
        },
        "RequestParameters": {
          "method.request.path.proxy": true
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "d84bf100-4b1d-4ce6-9d53-62e6ba2fbbbf"
        }
      },
      "DependsOn": [
        "LambdaFunction"
      ]
    },
    "LambdaIamRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "RoleName": {
          "Fn::Sub": "ThreatStackServiceLambda-${AppName}"
        },
        "Path": "/service-role/",
        "ManagedPolicyArns": [
          {
            "Ref": "SnsConfirmIamManagedPolicy"
          }
        ],
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": "lambda.amazonaws.com"
              },
              "Action": "sts:AssumeRole"
            }
          ]
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "d2334711-6f09-4e7d-b6bd-1c1820e89ff4"
        }
      }
    },
    "LambdaIamPolicy": {
      "Type": "AWS::IAM::Policy",
      "Properties": {
        "PolicyName": "LambdaExecutionPolicy",
        "Roles": [
          {
            "Ref": "LambdaIamRole"
          }
        ],
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": "logs:CreateLogGroup",
              "Resource": {
                "Fn::Sub": "arn:aws:logs:${AWS::Region}:${AWS::AccountId}:*"
              }
            },
            {
              "Effect": "Allow",
              "Action": [
                "sns:ListTopics"
              ],
              "Resource": [
                {
                  "Fn::Sub": "arn:aws:sns:${AWS::Region}:${AWS::AccountId}:*"
                }
              ]
            },
            {
              "Effect": "Allow",
              "Action": [
                "sns:GetTopicAttributes",
                "sns:Publish"
              ],
              "Resource": [
                {
                  "Fn::Sub": "arn:aws:sns:${AWS::Region}:${AWS::AccountId}:${ThreatStackServiceIntegrationsSnsTopicName}"
                }
              ]
            },
            {
              "Effect": "Allow",
              "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
              ],
              "Resource": [
                {
                  "Fn::Sub": "arn:aws:logs:${AWS::Region}:${AWS::AccountId}:log-group:/aws/lambda/${AppName}:*"
                }
              ]
            }
          ]
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "683b3f20-3945-46dc-8e96-af3ad3a2bb13"
        }
      }
    },
    "LambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "AppS3Bucket"
          },
          "S3Key": {
            "Ref": "AppS3Key"
          }
        },
        "Description": "Threat Stack to AWS SNS",
        "Environment": {
          "Variables": {
            "TS_AWS_SNS_TOPIC": {
              "Fn::GetAtt": [
                "SnsTopic",
                "TopicName"
              ]
            }
          }
        },
        "FunctionName": {
          "Ref": "AppName"
        },
        "Handler": {
          "Ref": "LambdaHandler"
        },
        "MemorySize": {
          "Ref": "LambdaMemorySize"
        },
        "Role": {
          "Fn::GetAtt": [
            "LambdaIamRole",
            "Arn"
          ]
        },
        "Runtime": "python2.7",
        "Timeout": {
          "Ref": "LambdaTimeout"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "293804a3-70b0-40fa-8d83-299e6432a7bc"
        }
      }
    },
    "LambdaPermission": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "FunctionName": {
          "Ref": "LambdaFunction"
        },
        "Action": "lambda:InvokeFunction",
        "Principal": "apigateway.amazonaws.com"
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "32857435-a44d-4602-a6ca-5f18a0b547bd"
        }
      },
      "Condition": ""
    },
    "RestApi": {
      "Type": "AWS::ApiGateway::RestApi",
      "Properties": {
        "Name": {
          "Ref": "ThreatStackRestApiName"
        },
        "Description": "Threat Stack service integrations"
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "4368da5f-f23e-4f12-a349-5d3df71c7b6d"
        }
      }
    },
    "UpdaterLambdaIamRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "RoleName": {
          "Fn::Sub": "ThreatStackServiceLambda-threatstack-integration-updater"
        },
        "Path": "/service-role/",
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": "lambda.amazonaws.com"
              },
              "Action": "sts:AssumeRole"
            }
          ]
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "c0a99a05-e85b-4000-9403-6f699d8b347f"
        }
      }
    },
    "UpdaterLambdaIamPolicy": {
      "Type": "AWS::IAM::Policy",
      "Properties": {
        "PolicyName": "UpdaterLambdaExecutionPolicy",
        "Roles": [
          {
            "Ref": "UpdaterLambdaIamRole"
          }
        ],
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": "logs:CreateLogGroup",
              "Resource": {
                "Fn::Sub": "arn:aws:logs:${AWS::Region}:${AWS::AccountId}:*"
              }
            },
            {
              "Effect": "Allow",
              "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
              ],
              "Resource": [
                {
                  "Fn::Sub": "arn:aws:logs:${AWS::Region}:${AWS::AccountId}:log-group:/aws/lambda/${UpdaterAppName}:*"
                }
              ]
            },
            {
              "Effect": "Allow",
              "Action": "lambda:UpdateFunctionCode",
              "Resource": [
                {
                  "Fn::Sub": "arn:aws:lambda:${AWS::Region}:${AWS::AccountId}:function:threatstack-to-*"
                },
                {
                  "Fn::Sub": "arn:aws:lambda:${AWS::Region}:${AWS::AccountId}:function:${UpdaterAppName}"
                }
              ]
            }
          ]
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "b03e3437-8bf6-46dc-90e7-c6955d73c0f1"
        }
      }
    },
    "UpdaterLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "AppS3Bucket"
          },
          "S3Key": {
            "Ref": "AppS3Key"
          }
        },
        "Description": "Threat Stack Integrations Updater.",
        "FunctionName": {
          "Ref": "UpdaterAppName"
        },
        "Handler": {
          "Ref": "UpdaterLambdaHandler"
        },
        "MemorySize":512,
        "Role": {
          "Fn::GetAtt": [
            "UpdaterLambdaIamRole",
            "Arn"
          ]
        },
        "Runtime": "python2.7",
        "Timeout":60
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "2e6470e4-71ee-4510-a252-def5ca3ddd46"
        }
      }
    },
    "UpdaterLambdaInvokeUpdater": {
      "Type": "Custom::LambdaUpdaterInvoke",
      "Properties": {
        "ServiceToken": {
          "Fn::Sub": "arn:aws:lambda:${AWS::Region}:${AWS::AccountId}:function:${UpdaterAppName}"
        },
        "FunctionName": {
          "Ref": "UpdaterAppName"
        },
        "FunctionZipFileUrl": {
          "Fn::Sub": "https://s3.amazonaws.com/${AppS3Bucket}/${AppS3Key}"
        }
      }
    },
    "LambdaInvokeUpdater": {
      "Type": "Custom::LambdaUpdaterInvoke",
      "Properties": {
        "ServiceToken": {
          "Fn::Sub": "arn:aws:lambda:${AWS::Region}:${AWS::AccountId}:function:${UpdaterAppName}"
        },
        "FunctionName": {
          "Ref": "AppName"
        },
        "FunctionZipFileUrl": {
          "Fn::Sub": "https://s3.amazonaws.com/${AppS3Bucket}/${AppS3Key}"
        }
      }
    },
    "UpdaterLambdaVersion": {
      "Type": "AWS::Lambda::Version",
      "Properties": {
        "FunctionName": {
          "Ref": "UpdaterLambdaFunction"
        },
        "CodeSha256": "%%SHA256%%"
      },
      "DependsOn": [
        "UpdaterLambdaInvokeUpdater"
      ]
    },
    "LambdaVersion": {
      "Type": "AWS::Lambda::Version",
      "Properties": {
        "FunctionName": {
          "Ref": "LambdaFunction"
        },
        "CodeSha256": "%%SHA256%%"
      },
      "DependsOn": [
        "LambdaInvokeUpdater"
      ]
    }
  },
  "Parameters": {
    "AppName": {
      "Type": "String",
      "Description": "Name of API.",
      "Default": "threatstack-to-aws-sns"
    },
    "AppEndPoint": {
      "Type": "String",
      "Description": "Path to Api endpoint",
      "Default": "/api/v1/sns/message"
    },
    "AppS3Bucket": {
      "Type": "String",
      "Description": "S3 bucket containing Lambda code.",
      "Default": "ts-demo-lamba-deploys"
    },
    "AppS3Key": {
      "Type": "String",
      "Description": "S3 bucket containing Lambda code.",
      "Default": "threatstack-to-aws-sns.zip"
    },
    "DeploymentStage": {
      "Type": "String",
      "Description": "Name of API deployment stage",
      "Default": "prod"
    },
    "LambdaHandler": {
      "Type": "String",
      "Description": "Lambda handler function name",
      "Default": "threatstack-to-aws-sns.application"
    },
    "LambdaMemorySize": {
      "Type": "String",
      "Description": "Lambda memory size",
      "Default": "128"
    },
    "LambdaTimeout": {
      "Type": "String",
      "Description": "Lambda timeout",
      "Default": "28"
    },
    "ThreatStackServiceIntegrationsSnsTopicName": {
      "Description": "SNS Topic name for service integrations",
      "Type": "String",
      "Default": "ThreatStackServiceIntegrations"
    },
    "ThreatStackRestApiName": {
      "Description": "SNS Topic name for service integrations",
      "Type": "String",
      "Default": "threatstack-svc-integrations"
    },
    "UpdaterAppName": {
      "Type": "String",
      "Description": "Name of integrations updater function.",
      "Default": "threatstack-integration-updater"
    },
    "UpdaterLambdaHandler": {
      "Type": "String",
      "Description": "Lambda integration updater handler function name",
      "Default": "threatstack-integration-updater.handler"
    }
  },
  "Outputs": {
    "LambdaVersionSha256": {
      "Description": "Sha256 of deployed code.",
      "Value": "%%SHA256%%"
    },
    "SnsConfirmIamManagedPolicy": {
      "Description": "Rest API Root Resource ID.",
      "Value": {
        "Ref": "SnsConfirmIamManagedPolicy"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-SnsConfirmIamManagedPolicy"
        }
      }
    },
    "SnsTopicName": {
      "Description": "Name of integrations Topic",
      "Value": {
        "Fn::GetAtt": [
          "SnsTopic",
          "TopicName"
        ]
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-SnsTopicName"
        }
      }
    },
    "SnsTopicArn": {
      "Description": "ARN of integrations Topic",
      "Value": {
        "Ref": "SnsTopic"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-SnsTopicArn"
        }
      }
    },
    "SnsWebHookEndpoint": {
      "Value": {
        "Fn::Join": [
          "",
          [
            "https://",
            {
              "Ref": "RestApi"
            },
            ".execute-api.",
            {
              "Ref": "AWS::Region"
            },
            ".amazonaws.com/",
            {
              "Ref": "DeploymentStage"
            },
            "/",
            {
              "Ref": "AppName"
            },
            {
              "Ref": "AppEndPoint"
            }
          ]
        ]
      }
    },
    "RestApiId": {
      "Description": "Rest API ID.",
      "Value": {
        "Ref": "RestApi"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-RestApiId"
        }
      }
    },
    "RootResourceId": {
      "Description": "Rest API Root Resource ID.",
      "Value": {
        "Fn::GetAtt": [
          "RestApi",
          "RootResourceId"
        ]
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-RootResourceId"
        }
      }
    },
    "UpdaterLambdaArn": {
      "Description": "Updater Lambda Function ARN.",
      "Value": {
        "Fn::GetAtt": ["UpdaterLambdaFunction", "Arn"]
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-UpdaterLambdaArn"
        }
      }
    }
  },
